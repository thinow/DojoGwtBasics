<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Servlets -->
  <servlet>
    <servlet-name>timeServlet</servlet-name>
    <servlet-class>dojo.gwt.server.TimeServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>timeServlet</servlet-name>
    <url-pattern>/DojoGwtBasics/time</url-pattern>
  </servlet-mapping>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>DojoGwtBasics.html</welcome-file>
  </welcome-file-list>

</web-app>
